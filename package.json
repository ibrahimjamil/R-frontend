{
  "name": "renteez",
  "version": "1.0.0",
  "description": "Renteez Reactjs app using nextjs.",
  "author": "Muaaz Tausif <muaaz.tausif@toptal.com>",
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "NODE_ENV=development env-cmd next",
    "export": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next export",
    "build": "NODE_ENV=local env-cmd -f .env --fallback --silent next build",
    "build:dev": "NODE_ENV=development env-cmd -f .env.dev --fallback --silent next build",
    "build:staging": "NODE_ENV=staging env-cmd -f .env.staging --fallback --silent next build",
    "build:prod": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next build",
    "start": "NODE_ENV=local env-cmd -f .env.local --fallback --silent next start",
    "start:dev": "NODE_ENV=development env-cmd -f .env.dev --fallback --silent next start",
    "start:staging": "NODE_ENV=staging env-cmd -f .env.staging --fallback --silent next start",
    "start:prod": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next start",
    "lint": "eslint src --ext .js,.jsx",
    "format": "eslint src/**/*.js --fix",
    "lint:next": "next lint",
    "analyze": "NODE_ENV=production ANALYZE=true env-cmd -f .env.production --fallback --silent next build",
    "serve": "serve .next/analyze",
    "start:pm2": "NODE_ENV=production pm2 start npm --name \"boilerplate\" -- start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.17.2",
    "antd-mask-input": "^0.1.15",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "frontmatter-markdown-loader": "^3.6.3",
    "lodash": "^4.17.21",
    "next": "^12.0.4",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "qs": "^6.10.3",
    "query-string": "^7.0.1",
    "rc-picker": "^2.5.19",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.8",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.4",
    "@svgr/webpack": "^6.1.2",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "env-cmd": "^10.1.0",
    "eslint": "<8.0.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-next": "12.0.4",
    "husky": "^7.0.4",
    "less-vars-to-js": "^1.3.0",
    "next-plugin-antd-less": "^1.5.2",
    "webpack": "^5.64.4"
  }
}
